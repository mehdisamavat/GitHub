<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary" />
            <corners
                android:bottomLeftRadius="@dimen/mRadius"
                android:bottomRightRadius="@dimen/mRadius"
                android:topLeftRadius="@dimen/mRadius"
                android:topRightRadius="@dimen/mRadius" />
            <stroke
                android:width="@dimen/minWidth"
                android:color="@color/secondaryVariant" />
        </shape>
    </item>

</layer-list>